{
  "family": "parlae-backend-migration-fix",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::234270344223:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "migration-fix",
      "image": "234270344223.dkr.ecr.us-east-2.amazonaws.com/parlae-backend:latest",
      "essential": true,
      "command": [
        "/bin/sh",
        "-c",
        "cd /app/packages/prisma && npx prisma migrate resolve --applied 20260209000000_add_vapi_phone_numbers && npx prisma migrate deploy && echo 'Migration fix complete!'"
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/aws/ecs/parlae-backend",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "migration-fix"
        }
      },
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        }
      ]
    }
  ]
}
